<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
		 <changeSet id="1" author="Ruban">
			<createTable tableName="clinic">
				<column name="id" type="int"
				autoIncrement="true">
				<constraints primaryKey="true" nullable="false"/>
				</column>
				<column name="labname" type="varchar(50)"/>
				<column name="lablocation" type="varchar(50)">
				<constraints nullable="false"/>
				</column>
			</createTable>
			</changeSet>
			<changeSet id="2" author="Ruban">
			<createTable tableName="Depart">
				<column name="deptid" type="int"
				autoIncrement="true">
				<constraints primaryKey="true" nullable="false"/>
				</column>
				<column name="deptname" type="varchar(50)"/>
				<column name="deptlocation" type="varchar(50)">
				<constraints nullable="false"/>
				</column>
			</createTable>
			</changeSet>
			<changeSet id="tag-2.1" author="Ruban">
				<tagDatabase tag="2.1" />
			</changeSet>
</databaseChangeLog>
